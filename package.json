{
	"name": "@untitled-theme/root",
	"private": true,
	"scripts": {
		"generate:icons:react": "pnpm --filter @untitled-theme/scripts run generate:icons:react",
		"generate:icons:svelte": "pnpm --filter @untitled-theme/scripts run generate:icons:svelte",
		"generate:icons": "pnpm --filter @untitled-theme/scripts run generate:icons",
		"release:icons:react": "pnpm --filter @untitled-theme/icons-react run release",
		"release:icons:svelte": "pnpm --filter @untitled-theme/icons-svelte run release",
		"release:icons": "pnpm run release:icons:react && pnpm run release:icons:svelte",
		"release:preset:panda": "pnpm --filter @untitled-theme/preset-panda run release"
	},
	"devDependencies": {
		"@types/node": "20.11.17",
		"prettier": "3.2.5",
		"typescript": "5.3.3"
	},
	"packageManager": "pnpm@8.15.1"
}
