{
  "name": "@untitled-theme/root",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "website": "pnpm --filter \"@untitled-theme/website\"",
    "scripts": "pnpm --filter \"@untitled-theme/scripts\"",
    "colors": "pnpm --filter \"@untitled-theme/colors\"",
    "icons-react": "pnpm --filter \"@untitled-theme/icons-react\"",
    "icons-svelte": "pnpm --filter \"@untitled-theme/icons-svelte\"",
    "icons-solid": "pnpm --filter \"@untitled-theme/icons-solid\"",
    "test": "pnpm --filter \"*\" test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "release:core": "pnpm --filter \"@untitled-theme/colors\"",
    "release:icons": "pnpm --filter \"@untitled-theme/icons-*\" release",
    "release": "pnpm --filter \"*\" release"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
}
