{
	"name": "@untitled-theme/preset-panda",
	"type": "module",
	"version": "0.0.1",
	"license": "MIT",
	"description": "Panda CSS Preset for Untitled UI",
	"keywords": ["Ark UI", "Panda CSS", "Preset", "Untitled UI"],
	"repository": "https://github.com/calvo-jp/untitled-theme",
	"bugs": {
		"url": "https://github.com/calvo-jp/untitled-theme/issues"
	},
	"author": {
		"name": "John Paul Calvo",
		"email": "calvojp92@gmail.com"
	},
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": ["dist"],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite",
		"lint": "biome lint .",
		"build": "vite build --mode library",
		"release": "release-it",
		"prepack": "clean-package",
		"postpack": "clean-package restore",
		"postinstall": "panda codegen"
	},
	"dependencies": {
		"@ark-ui/anatomy": "3.4.0",
		"@untitled-theme/colors": "0.3.1"
	},
	"devDependencies": {
		"@ark-ui/react": "3.5.0",
		"@pandacss/dev": "0.41.0",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@untitled-theme/icons-react": "0.10.1",
		"@vitejs/plugin-react-swc": "3.7.0",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router-dom": "6.24.1",
		"release-it": "17.4.1",
		"typescript": "5.5.3",
		"vite": "5.3.3",
		"vite-plugin-dts": "3.9.1",
		"vite-tsconfig-paths": "4.3.2"
	},
	"peerDependencies": {
		"@ark-ui/react": ">=3.0.0",
		"@pandacss/dev": ">=0.22.0"
	},
	"peerDependenciesMeta": {
		"@ark-ui/react": {
			"optional": true
		}
	},
	"release-it": {
		"hooks": {
			"after:bump": ["pnpm build"]
		},
		"git": {
			"commitMessage": "chore: release ${npm.name} v${version}",
			"tagName": "${npm.name}@${version}"
		},
		"github": {
			"release": false
		}
	}
}
