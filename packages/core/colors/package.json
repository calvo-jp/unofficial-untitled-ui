{
	"name": "@untitled-theme/colors",
	"type": "module",
	"version": "0.3.0",
	"license": "MIT",
	"description": "Untitled UI colors for Tailwind and Panda CSS",
	"keywords": [
		"Untitled UI",
		"Tailwind",
		"Panda CSS",
		"colors"
	],
	"repository": "https://github.com/calvo-jp/untitled-theme",
	"bugs": {
		"url": "https://github.com/calvo-jp/untitled-theme/issues"
	},
	"author": {
		"name": "John Paul Calvo",
		"email": "calvojp92@gmail.com"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./panda": {
			"types": "./dist/panda.d.ts",
			"import": "./dist/panda.js"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "vite build",
		"release": "release-it"
	},
	"devDependencies": {
		"@tsconfig/node20": "20.1.4",
		"release-it": "17.3.0",
		"typescript": "5.4.5",
		"vite": "5.2.13",
		"vite-plugin-dts": "3.9.1"
	},
	"release-it": {
		"hooks": {
			"after:bump": [
				"pnpm build"
			]
		},
		"git": {
			"commitMessage": "chore: release ${npm.name} v${version}",
			"tagName": "${npm.name}@${version}"
		},
		"github": {
			"release": false
		}
	}
}
