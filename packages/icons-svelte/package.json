{
	"name": "@untitled-theme/icons-svelte",
	"type": "module",
	"version": "0.0.12",
	"license": "MIT",
	"description": "Untitled UI Icons for Svelte",
	"keywords": [
		"svg",
		"icon",
		"icons",
		"svelte",
		"untitled ui"
	],
	"repository": "https://github.com/calvo-jp/untitled-theme",
	"bugs": {
		"url": "https://github.com/calvo-jp/untitled-theme/issues"
	},
	"main": "dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"svelte": "./dist/*.svelte"
		},
		"./package.json": {
			"default": "./package.json"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prebuild": "pnpm --filter @untitled-theme/scripts run generate:icons -f svelte",
		"build": "svelte-package --input ./src",
		"release": "release-it"
	},
	"devDependencies": {
		"@sveltejs/kit": "2.5.0",
		"@sveltejs/package": "2.2.6",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@tsconfig/svelte": "5.0.2",
		"release-it": "17.0.5",
		"svelte": "5.0.0-next.53",
		"svelte-check": "3.6.4",
		"svelte-preprocess": "5.1.3",
		"typescript": "5.3.3",
		"vite": "5.1.1"
	},
	"peerDependencies": {
		"svelte": ">=5.0.0"
	},
	"release-it": {
		"hooks": {
			"after:bump": [
				"pnpm build"
			]
		},
		"git": {
			"commitMessage": "chore: release ${npm.name} v${version}",
			"tagName": "${npm.name}@${version}"
		},
		"github": {
			"release": false
		}
	}
}
