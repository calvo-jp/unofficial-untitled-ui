{
	"name": "@untitled-theme/icons-react",
	"version": "0.0.6",
	"license": "MIT",
	"description": "Untitled UI Icons for React",
	"keywords": [
		"react",
		"icons",
		"untitled ui"
	],
	"main": "src/index.ts",
	"files": [
		"src",
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsup",
		"release": "release-it",
		"prepack": "clean-package",
		"postpack": "clean-package restore"
	},
	"devDependencies": {
		"@types/react": "18.2.55",
		"clean-package": "2.2.0",
		"react": "18.2.0",
		"release-it": "17.0.5",
		"tsup": "8.0.2",
		"typescript": "5.3.3"
	},
	"peerDependencies": {
		"react": ">18"
	},
	"tsup": {
		"dts": true,
		"clean": true,
		"entry": [
			"src/index.ts"
		],
		"format": [
			"esm",
			"cjs"
		],
		"external": [
			"react"
		],
		"sourcemap": true
	},
	"clean-package": {
		"replace": {
			"main": "dist/index.js",
			"module": "dist/index.mjs",
			"types": "dist/index.d.ts",
			"exports": {
				".": {
					"types": "./dist/index.d.ts",
					"import": "./dist/index.mjs",
					"require": "./dist/index.js"
				},
				"./package.json": "./package.json"
			}
		}
	},
	"release-it": {
		"hooks": {
			"after:bump": [
				"pnpm build"
			]
		},
		"git": {
			"commitMessage": "chore: release ${npm.name} v${version}",
			"tagName": "${npm.name}@${version}"
		},
		"github": {
			"release": false
		}
	}
}
