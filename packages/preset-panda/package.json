{
	"name": "@untitled-theme/preset-panda",
	"type": "module",
	"version": "0.0.3",
	"license": "MIT",
	"description": "Untitled UI preset for Panda and Ark UI",
	"keywords": [
		"untitled ui",
		"preset",
		"ark ui",
		"panda"
	],
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.esm.js",
			"require": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"debug": "vite",
		"build": "vite build",
		"release": "release-it"
	},
	"dependencies": {
		"@ark-ui/anatomy": "2.2.0",
		"@untitled-theme/colors": "0.0.2"
	},
	"devDependencies": {
		"@ark-ui/react": "2.1.1",
		"@fontsource/inter": "5.0.16",
		"@fontsource/open-sans": "5.0.24",
		"@pandacss/dev": "0.32.1",
		"@types/react": "18.2.58",
		"@types/react-dom": "18.2.19",
		"@typescript-eslint/eslint-plugin": "7.0.2",
		"@typescript-eslint/parser": "7.0.2",
		"@untitled-theme/icons-react": "workspace:*",
		"@vitejs/plugin-react-swc": "3.6.0",
		"eslint": "8.57.0",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-react-refresh": "0.4.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.22.1",
		"release-it": "17.1.1",
		"typescript": "5.3.3",
		"vite": "5.1.4",
		"vite-plugin-dts": "3.7.3"
	},
	"peerDependencies": {
		"@pandacss/dev": ">=0.32.0"
	},
	"release-it": {
		"hooks": {
			"after:bump": [
				"pnpm build"
			]
		},
		"git": {
			"commitMessage": "chore: release ${npm.name} v${version}",
			"tagName": "${npm.name}@${version}"
		},
		"github": {
			"release": false
		}
	}
}
