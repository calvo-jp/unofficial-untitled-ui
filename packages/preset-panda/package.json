{
	"name": "@untitled-theme/preset-panda",
	"version": "0.0.1",
	"license": "MIT",
	"description": "Untitled UI preset for Panda and Ark UI",
	"keywords": [
		"untitled ui",
		"preset",
		"ark ui",
		"panda"
	],
	"main": "src/index.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsup",
		"release": "release-it",
		"prepack": "clean-package",
		"postpack": "clean-package restore"
	},
	"devDependencies": {
		"@ark-ui/anatomy": "2.2.0",
		"@pandacss/dev": "0.31.0",
		"clean-package": "2.2.0",
		"release-it": "17.0.5",
		"tsup": "8.0.2"
	},
	"peerDependencies": {
		"@pandacss/dev": ">0.31.0"
	},
	"clean-package": {
		"replace": {
			"main": "dist/index.js",
			"module": "dist/index.mjs",
			"types": "dist/index.d.ts",
			"exports": {
				".": {
					"types": "./dist/index.d.ts",
					"import": "./dist/index.mjs",
					"require": "./dist/index.js"
				},
				"./package.json": "./package.json"
			}
		}
	}
}
