{
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
  },
  "eslint.workingDirectories": ["website"],
  "files.associations": {
    "**/website/**/*.css": "tailwindcss"
  },
  "tailwindCSS.experimental.classRegex": [
    ["twJoin\\(([^)]*)\\)", "'([^']*)'"],
    ["twMerge\\(([^)]*)\\)", "'([^']*)'"]
  ]
}
